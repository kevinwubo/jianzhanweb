@using Entity.ViewModel;
@using Common;
@{
    ViewBag.Title = "销售转移监控列表";
    List<InquiryMonitorEntity> list = (List<InquiryMonitorEntity>)ViewBag.InquiryMonitor;
    PagerInfo p = (PagerInfo)ViewBag.Pager;
}
<div class="row-fluid">
    <h2 class="heading" style="margin-top:50px">销售转移监控列表</h2>
</div>
<div class="row-fluid">
    <div class="widget widget-padding span12">
        <div class="widget-header">
            <i class="icon-laptop"></i>
            <h5>销售转移监控列表</h5>
        </div>
        <div class="widget-body" style="margin-bottom:50px">
            <div id="menus_wrapper" class="dataTables_wrapper form-inline" role="grid">
                <div class="row-fluid">
                    <div class="">
                        
                    </div>
                </div>
                @if (list != null && list.Count > 0)
                {
                    <table id="users" class="table table-striped table-bordered dataTable" aria-describedby="users_info">
                        <thead>
                            <tr role="row">
                                <th role="columnheader" tabindex="0" rowspan="1" colspan="1" >选择</th>
                                <th role="columnheader" tabindex="0" rowspan="1" colspan="1" >产品编号</th>
                                <th role="columnheader" tabindex="0" rowspan="1" colspan="1" >原始销售姓名</th>
                                <th role="columnheader" tabindex="0" rowspan="1" colspan="1" >转移后销售姓名</th>
                                <th role="columnheader" tabindex="0" rowspan="1" colspan="1" >操作</th>
                            </tr>
                        </thead>

                        <tbody role="alert" aria-live="polite" aria-relevant="all">
                            @foreach (InquiryMonitorEntity entity in list)
                            {
                                <tr>
                                    <td><input type="checkbox" value="@entity.ID" /></td>
                                    <td>@entity.ProductID</td>
                                    <td>@entity.OriginSalesName</td>
                                    <td>@entity.NewSalesName</td>
                                    <td>@entity.ProductID</td>
                                    <td class=" ">
                                        <a href="###"><i class="icon-edit"></i> 处理</a>
                                    </td>
                                </tr>
                            }

                        </tbody>
                    </table>
                }
                @{ Html.RenderPartial("../Pager", p); }
            </div>
        </div> <!-- /widget-body -->
    </div> <!-- /widget -->
</div>
@section Footer{
<script type="text/javascript" src="~/source/assets/js/viewer-jquery.min.js"></script>
<script src="~/source/layui/layui.js"></script>

}